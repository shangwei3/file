
/*
 * Device tree overlay for uart8
 */

#include "dt-bindings/gpio/gpio.h"
#include "dt-bindings/pinctrl/stm32-pinfunc.h"

/dts-v1/;
/plugin/;

/ {
     compatible = "100ask,100ask-stm32mp1", "st,stm32mp157";
    fragment@0 {
        target = <&pinctrl>;
        __overlay__ {
			m4_uart8_pins_mx: m4_uart8_mx-0 {
				pins {
					pinmux = <STM32_PINMUX('E', 0, RSVD)>, /* UART8_RX */
							 <STM32_PINMUX('E', 1, RSVD)>; /* UART8_TX */
				};
			};
        };
    };
	
    fragment@1 {
        target = <&uart8>;
        __overlay__ {
			status = "disable";
        };
    };
	
		fragment@2 {
        target = <&m4_uart8>;
        __overlay__ {
			pinctrl-names = "default";
			pinctrl-0 = <&m4_uart8_pins_mx>;
			status = "okay";
        };
    };
};

